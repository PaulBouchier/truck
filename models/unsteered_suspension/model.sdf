<?xml version="1.0" ?>

<sdf version="1.5">
  <model name="unsteered_suspension">

<!-- Inner Chassis Mount -->
    <link name="inner_mount">
      <pose>0 0.025 1.35  0 0 0</pose>
      <inertial>
        <pose>0 0.025 1.35  0 0 0</pose>
        <mass>10</mass>
        <inertia>
          <ixx>10</ixx>
          <iyy>10</iyy>
          <izz>10</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <box>
            <size>0.1 0.05 0.2</size>
          </box>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <box>
            <size>0.1 0.05 0.2</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Grey</name>
          </script>
        </material>
      </visual>
    </link>

<!-- upper arm -->
    <link name="upper_suspension_arm">
      <pose>0 0.35 1.45  -1.5707963267948966 0 0</pose>
      <inertial>
        <mass>20</mass>
        <inertia>
          <ixx>20</ixx>
          <iyy>20</iyy>
          <izz>20</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.6</length>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.6</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Grey</name>
          </script>
        </material>
      </visual>
    </link>

    <joint name="inner_moount_to_upper_arm" type="revolute">
      <parent>inner_mount</parent>
      <child>upper_suspension_arm</child>
      <pose>0 0 -0.3 0 0 0</pose>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <upper>0.5</upper>
          <lower>-0.5</lower>
        </limit>
        <dynamics>
          <damping>10000</damping>
          <spring_stiffness>100000</spring_stiffness>
          <spring_reference>-0.5</spring_reference>
        </dynamics>
      </axis>
    </joint>

    <joint name="wheel_moount_to_upper_arm" type="revolute">
      <parent>wheel_mount</parent>
      <child>upper_suspension_arm</child>
      <pose>0 0 0.3 0 0 0</pose>
      <axis>
        <xyz>1 0 0</xyz>
      </axis>
    </joint>

<!-- lower arm -->
    <link name="lower_suspension_arm">
      <pose>0 0.35 1.25  -1.5707963267948966 0 0</pose>
      <inertial>
        <mass>20</mass>
        <inertia>
          <ixx>20</ixx>
          <iyy>20</iyy>
          <izz>20</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.6</length>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.6</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Grey</name>
          </script>
        </material>
      </visual>
    </link>

    <joint name="inner_moount_to_lower_arm" type="revolute">
      <parent>inner_mount</parent>
      <child>lower_suspension_arm</child>
      <pose>0 0 -0.3 0 0 0</pose>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <upper>0.5</upper>
          <lower>-0.5</lower>
        </limit>
        <dynamics>
          <damping>0.5</damping>
        </dynamics>
      </axis>
    </joint>

    <joint name="wheel_moount_to_lower_arm" type="revolute">
      <parent>wheel_mount</parent>
      <child>lower_suspension_arm</child>
      <pose>-0 0 0.3 0 0 0</pose>
      <axis>
        <xyz>1 0 0</xyz>
      </axis>
    </joint>

<!-- Wheel Mount -->
    <link name="wheel_mount">
      <pose>0 0.675 1.35  0 0 0</pose>
      <inertial>
        <mass>10</mass>
        <inertia>
          <ixx>10</ixx>
          <iyy>10</iyy>
          <izz>10</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <box>
            <size>0.05 0.05 0.2</size>
          </box>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <box>
            <size>0.05 0.05 0.2</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Grey</name>
          </script>
        </material>
      </visual>
    </link>

<!-- Wheel -->
    <link name="wheel_unsteered">
      <pose>0 1.3575 1.4  -1.5707963267948966 0 0</pose>
      <inertial>
        <mass>2000</mass>
        <inertia>
          <ixx>100</ixx>
          <iyy>100</iyy>
          <izz>100</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>1.35</radius>
            <length>1.25</length>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>1.35</radius>
            <length>1.25</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Wood</name>
          </script>
        </material>
      </visual>
    </link>

    <joint type="revolute" name="unsteered_wheel_joint">
      <parent>wheel_mount</parent>
      <child>wheel_unsteered</child>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
      <physics>
        <ode>
          <limit>
            <cfm>0.0</cfm>
            <erp>0.9</erp>
          </limit>
        </ode>
      </physics>
    </joint>

  </model>
</sdf>
